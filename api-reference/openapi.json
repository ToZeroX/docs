              }
            }
          }
        }
      },
      "Pagination": {
        "type": "object",
        "properties": {
          "pagination_before": {
            "type": "string",
            "description": "上一页游标（用于获取更早的事件）。为空时表示已经到达起点。",
            "example": "eyJpZCI6IjEyMyJ9"
          },
          "pagination_after": {
            "type": "string",
            "description": "下一页游标（用于获取更新的事件）。为空时表示已经到达终点。",
            "example": "eyJpZCI6IjQ1NiJ9"
          },
          "pagination_total_count": {
            "type": "integer",
