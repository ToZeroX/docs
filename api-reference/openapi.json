                "enum": [
                  "Submitted", "PendingScreening", "PendingAuthorization", "PendingSignature", "Broadcasting", "Confirming", "Completed", "Failed", "Rejected", "Pending"
                ]
              },
              "type": {
                "type": "string",
                "description": "事件类型， **Deposit** 为充值",
                "enum": [
                  "Deposit", "Withdrawal", "ContractCall", "MessageSign", "ExternalSafeTx", "Stake", "Unstake"
