                    "enum": [
                      "Address", "CustodialWallet", "ExchangeWallet", "EVM_Contract", "SOL_Contract", "COSMOS_Contract", "EVM_EIP_191_Signature", "EVM_EIP_712_Signature", "BTC_BIP_137_Signature", "BTC_BIP_322_Signature", "COSMOS_ADR_36_Signature", "Raw_Message_Signature", "DepositToAddress", "DepositToWallet"
                    ]
                  },
                  "wallet_id": {
